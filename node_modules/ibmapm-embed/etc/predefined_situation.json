[{
        "thresholds": [{
            "severity": 4,
            "condition": {
                "$and": [{
                        "${metrics.averageResponseTime}": {
                            "$gte": 2000
                        }
                    },
                    {
                        "${metrics.averageResponseTime}": {
                            "$lt": 5000
                        }
                    }
                ]
            },
            "counter": {
                "$latest": 3,
                "$counter": 3
            },
            "history": {
                "$items": {
                    "averageResponseTime": "${metrics.averageResponseTime}",
                    "timestamp": "${timestamp}"
                },
                "$size": 1
            }
        }],
        "subject": [{
            "id": "${resourceID}",
            "originator": "emitter",
            "type": [
                "resource"
            ]
        }],
        "label": "Node.js_Average_Response_Time_High",
        "evaluationPoint": "rte",
        "resourceType": "nodeApplicationRuntime",
        "version": "0.8"
    },
    {
